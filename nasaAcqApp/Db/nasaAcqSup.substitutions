file "perSysmonLI.template" {
  pattern { P,   R,      PORT,                    NAME, IDX,       EGU,        LOLO,         LOW,        HIGH,        HIHI, DESC }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE), "FPGA:powerUp_",  0,        "",           0,           0,           2,           2, "FPGA has powered upregister" }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),   "ACQ:status",  92,        "", -2147483648, -2147483648,           1,           1, "Acquisition status register" }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),   "ACQ:active",  96,        "",           0,           0,           0,           0, "Active ADC channels" }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),"ACQ:DCcoupled", 100,        "",           0,           0,           0,           0, "DC-coupled ADC channels" }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),   "ACQ:clkPLL", 104,        "",           0,           0,           0,           0, "Acquisition clock PLL status" }
}

