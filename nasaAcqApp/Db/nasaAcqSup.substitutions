  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),  "DRDY:status", 148,   "",    0,   0,    0,    0, NO_ALARM, NO_ALARM, NO_ALARM, NO_ALARM, "HI:PPS->DRDY  LO:DRDYs skew" }
