  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),  "FPGA:uptime",   4,  "s",    0,   0,    0,    2, NO_ALARM, NO_ALARM, NO_ALARM, NO_ALARM, "Seconds since boot" }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE),   "ACQ:status", 132,   "",    0,   0,    0,    0, NO_ALARM, NO_ALARM, NO_ALARM, NO_ALARM, "Acquisition status register" }
