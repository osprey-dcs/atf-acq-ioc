file "perFPGA.template" {
  pattern { P,   R,      PORT }
  { \$(P),"\$(NODE):",\$(PORT)\$(NODE), }
}

file "perBuildDate.template"
{ pattern
{       P,          R,        PORT,                           NAME, IDX }
  { "\$(P)", "\$(NODE):", "\$(PORT)\$(NODE)",  "firmwareBuildDate",   0 }
  { "\$(P)", "\$(NODE):", "\$(PORT)\$(NODE)",  "softwareBuildDate",   1 }
}

