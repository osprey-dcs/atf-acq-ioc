file "perSysmonLI.template" {
  pattern { P R,PORT,                 NAME, IDX,       EGU,        LOLO,         LOW,        HIGH,        HIHI, DESC }
  { \$(P),"01:",\$(PORT)01,   "ACQ:status",  88,        "", -2147483648, -2147483648,           1,           1, "Acquisition status register" }
  { \$(P),"01:",\$(PORT)01,   "ACQ:active",  92,        "",           0,           0,           0,           0, "Active ADC channels" }
  { \$(P),"01:",\$(PORT)01,"ACQ:DCcoupled",  96,        "",           0,           0,           0,           0, "DC-coupled ADC channels" }
  { \$(P),"01:",\$(PORT)01,   "ACQ:clkPLL", 100,        "",           0,           0,           0,           0, "Acquisition clock PLL status" }
# And repeat 32 times, one for each node
}

file "perChannel.template" {
